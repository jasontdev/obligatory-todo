{
  "rules": {
    ".read": false,
    ".write": false,
    "users": {
      "$user_id": {
        ".write": "$user_id === auth.uid",
        ".read": "$user_id === auth.uid"
      }
    },
    "items": {
      "$item_id": {
        ".read": "data.child('user').val() === auth.uid",
        ".write": "newData.child('user').val() === auth.uid || data.child('user').val() === auth.uid"
      }
    }
  }
}
